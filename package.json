{
  "name": "incomplete-niwa-archives",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nuxt",
    "prebuild": "rm -rf dist",
    "build": "nuxt build",
    "pregenerate": "rm -rf dist",
    "generate": "nuxt generate",
    "rename": "mv ./dist/htaccess ./dist/.htaccess; mv ./dist/htpasswd ./dist/.htpasswd",
    "rename-back": "mv ./dist/.htaccess ./dist/htaccess; mv ./dist/.htpasswd ./dist/htpasswd",
    "deploy:staging": "sh deploy.sh staging",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "dependencies": {
    "@karappo-inc/vue-components": "^0.3.1",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/pwa": "^3.0.2",
    "core-js": "^3.6.5",
    "node-sass": "^5.0.0",
    "nuxt": "^2.14.6",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^10.1.0",
    "swiper": "5.4.5",
    "vue-awesome-swiper": "^4.1.1",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2"
  }
}
